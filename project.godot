; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="inkgd"
run/main_scene="res://examples/scenes/main.tscn"
config/features=PackedStringArray("4.0", "C#")
boot_splash/bg_color=Color(0.929412, 0.898039, 0.854902, 1)
boot_splash/image="res://examples/images/transparent.png"
boot_splash/fullsize=false
config/icon="res://icons/icon.png"

[autoload]

InkRuntime="*res://addons/inkgd/runtime.gd"

[debug]

gdscript/warnings/exclude_addons=false
gdscript/warnings/shadowed_variable=0
gdscript/warnings/assert_always_false=0
gdscript/completion/autocomplete_setters_and_getters=true

[display]

window/stretch/mode="2d"
window/stretch/aspect="expand"
window/size/width=1920
window/size/height=1080

[dotnet]

project/assembly_name="inkgd"

[editor_plugins]

enabled=PackedStringArray("res://addons/inkgd/plugin.cfg", "res://addons/gut/plugin.cfg")

[inkgd]

do_not_use_mono_runtime=true

[network]

limits/debugger_stdout/max_chars_per_second=600000

[rendering]

renderer/rendering_method="mobile"
environment/default_environment="res://default_env.tres"
